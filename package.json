{
  "name": "catalog-zero",
  "version": "1.0.4",
  "description": "Component library that supports customizable theming and styling",
  "main": "/dist/catalog-one.bundle.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "start": "webpack-dev-server --config ./webpack.dev.config.js --mode development",
    "build": "webpack --config ./webpack.prod.config.js --mode production",
    "storybook": "start-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tri4Ty/CatalogZero.git"
  },
  "keywords": [
    "React",
    "Component Library",
    "Customized Theming"
  ],
  "author": "Tri4Ty",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tri4Ty/CatalogZero/issues"
  },
  "homepage": "https://github.com/Tri4Ty/CatalogZero#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "babel-plugin-styled-components"
    ]
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/react": "^6.5.15",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^5.5.0",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.1",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "lodash": ">=4.17.13",
    "styled-components": "^4.3.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
