{
  "name": "catalog-zero",
  "version": "1.0.0",
  "description": "Component library that supports customizable theming and styling",
  "main": "/dist/catalog-one.bundle.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "start": "webpack-dev-server --config ./webpack.dev.config.js --mode development",
    "build": "webpack --config ./webpack.prod.config.js --mode production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tri4Ty/CatalogZero.git"
  },
  "keywords": [
    "React",
    "Component Library",
    "Customized Theming"
  ],
  "author": "Tri4Ty",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tri4Ty/CatalogZero/issues"
  },
  "homepage": "https://github.com/Tri4Ty/CatalogZero#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
